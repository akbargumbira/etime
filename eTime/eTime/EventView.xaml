<UserControl x:Class="eTime.EventView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}"
    d:DesignHeight="Auto" d:DesignWidth="480">
    
    <Grid x:Name="LayoutRoot">
        <StackPanel Background="Gray">
            <TextBlock Name="textBlockTitle" Text="title" FontSize="30"/>
            <TextBlock Name="textBlockStart"    Text="mulai" />
            <TextBlock Name="textBlockEnd"      Text="sampai" />
            <TextBlock Name="textBlockLocation" Text="Location" FontSize="25"/>
            <TextBlock Text=" "/>
        </StackPanel>
    </Grid>
</UserControl>
